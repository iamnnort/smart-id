{
  "name": "@iamnnort/smart-id",
  "version": "1.2.1",
  "description": "ID generator for Node.js - Unique - Smart - Short",
  "keywords": [
    "id",
    "uid",
    "uuid",
    "guid",
    "identifier",
    "random",
    "generator",
    "unique",
    "crypto"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "ts-node src/index",
    "build": "tsup",
    "format": "prettier --write '**/*.{ts,js,json}'",
    "lint": "eslint --fix",
    "unit": "jest",
    "test": "yarn lint && yarn unit",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamnnort/smart-id.git"
  },
  "author": "Nikita Pavets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamnnort/smart-id/issues"
  },
  "homepage": "https://github.com/iamnnort/smart-id#readme",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/qs": "^6.9.16",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.1"
  }
}
